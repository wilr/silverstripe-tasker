{
  "name": "wilr/silverstripe-tasker",
  "type": "silverstripe-vendormodule",
  "keywords": ["silverstripe"],
  "homepage": "https://github.com/wilr/silverstripe-tasker",
  "license": "BSD-3-Clause",
  "authors": [{
    "name": "Will Rossiter",
    "email": "will@fullscreen.io"
  }],
  "require": {
    "silverstripe/framework": "^4",
    "masterminds/html5": "^2.3"
  },
  "require-dev": {
      "phpunit/phpunit": "^5.7",
      "squizlabs/php_codesniffer": "^3"
  },
  "extra": {
      "branch-alias": {
          "dev-master": "1.x-dev"
      }
  },
  "autoload": {
      "psr-4": {
          "Wilr\\SilverStripe\\Tasker\\": "src/"
      }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
